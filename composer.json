{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "codedge/laravel-fpdf": "^1.12.3",
        "fakerphp/faker": "^1.24.1",
        "filament/filament": "^3.3.7",
        "inertiajs/inertia-laravel": "^2.0.1",
        "laravel/framework": "^12.4.1",
        "laravel/sanctum": "^4.0.8",
        "laravel/telescope": "^5.6",
        "laravel/tinker": "^2.10.1",
        "propaganistas/laravel-phone": "^5.3.6",
        "resend/resend-laravel": "^0.16.1",
        "spatie/laravel-medialibrary": "^11.12.9",
        "stancl/tenancy": "^3.9.1",
        "tightenco/ziggy": "^2.5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.2",
        "kitloong/laravel-migrations-generator": "^7.1",
        "larastan/larastan": ">=3.2",
        "laravel/breeze": "^2.3.6",
        "laravel/pint": "^1.21.2",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.7",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.1",
        "rector/rector": "^2.0.11",
        "xammie/mailbook": "^1.9"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:refactor": "rector --dry-run",
        "test:lint": "pint --test",
        "test:unit": "pest --testsuite Unit --parallel --coverage",
        "test:types": "phpstan",
        "test": [
            "@test:unit",
            "@test:lint",
            "@test:refactor"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
